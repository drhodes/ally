-- Is there a primitive on both sides?
A (+) B -> ( sum A B )


A (*) B -> ( mult A B )
A (*) (B ? C) -> ( A * B ? A * C )

A (*) ( -> A (*) (
A (*) B -> ( mult A B )
A (*) ( B ? C ) -> (A * B ? A * C)

A (*) (B _ C)
L (+) R -> (L + R)
_ (+) R -> (Prim, Prim) -> (mult L R)

({ _ }) ->


(*) > (+)
(*) | (L, R) -> (L * R)
(*) | (Prim, Prim) -> (mult L R)


L (*) R -> (L*R)
L (*) (L.R) -> (L*L . L*R)

(/) == (*)
(/) | (L, R) -> (L / R)
