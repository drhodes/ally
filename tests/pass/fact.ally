module math {
    def min1 (n) {
        return min n 1;
    }
    
    def compose(f, g) {
        f >> (a,b,c,d,e);
    }
    
    def gt1(val){
        return gt (val, 1);
    }
    
    def fact (n) {
        var tmp = 1;
        
        min1 <> |= n > mul <> tmp > dup > return
                |> |= gt1
                   |+> min1
                   |-> dup;
    }   
}
