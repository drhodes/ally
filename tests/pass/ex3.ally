
module ex3 {
	def filter(f, xs){
		var init = xs;
		var accum = list;
	
		init > head > |= list
                      |> f +> append <> accum > |= id.2
                                                |> return;
 
		init > tail > |= list 
                      |> id.1 > init 
                      |> |= emptyP 
                         |+> id.2 
                         |-> id.1;
	}
}
